# Neon PostgreSQL Database Configuration Template
# Copy this file to .env and fill in your actual database credentials
# IMPORTANT: .env should NEVER be committed to version control!

# Database Host (Neon connection pooler endpoint)
DB_HOST=your-neon-host.c-4.us-east-1.aws.neon.tech

# Database Port
DB_PORT=5432

# Database Name
DB_NAME=neondb

# Database User
DB_USER=neondb_owner

# Database Password
# WARNING: Keep this secret! Never share or commit this to version control.
DB_PASSWORD=your_secure_password_here

# Application Environment
APP_ENV=development
APP_DEBUG=true
